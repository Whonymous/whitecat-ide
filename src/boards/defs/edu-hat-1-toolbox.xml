  <category name="Display" colour="#4a6cd4">
	<block type="display_init" colour="#4a6cd4">
		<field name="CHIPSET">gdisplay.ST7735G_144</field>
		<field name="ORIENTATION">gdisplay.LANDSCAPE</field>
		<field name="FRAME_BUFFER">FALSE</field>
	</block>
	<block type="display_on" colour="#4a6cd4"></block>
	<block type="display_off" colour="#4a6cd4"></block>
	<block type="display_width" colour="#4a6cd4"></block>
	<block type="display_height" colour="#4a6cd4"></block>
	<block type="display_orient" colour="#4a6cd4"><field name="ORIENTATION">gdisplay.LANDSCAPE</field></block>
	<block type="display_clear" colour="#4a6cd4"><value name="COLOR"><shadow type="display_color_sel"><field name="COLOR">#ff0000</field></shadow></value></block>
	<block type="display_color_sel" colour="#4a6cd4"><field name="COLOR">#ff0000</field></block>
	<block type="display_color" colour="#4a6cd4">
	  <value name="R"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	  <value name="G"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	  <value name="B"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	</block>
	<block type="display_select_foreground" colour="#4a6cd4"><value name="COLOR"><shadow type="display_color_sel"><field name="COLOR">#ff0000</field></shadow></value></block>
	<block type="display_set_background" colour="#4a6cd4"><value name="COLOR"><shadow type="display_color_sel"><field name="COLOR">#ff0000</field></shadow></value></block>
	<block type="display_draw_pixel" colour="#4a6cd4">
	  <value name="X"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	  <value name="Y"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	  <value name="COLOR"><shadow type="display_color_sel"><field name="COLOR">#ff0000</field></shadow></value>
	</block>
	<block type="display_draw_line" colour="#4a6cd4">
	  <value name="X1"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	  <value name="Y1"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	  <value name="X2"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	  <value name="Y2"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	  <value name="COLOR"><shadow type="display_color_sel"><field name="COLOR">#ff0000</field></shadow></value>
	</block>
	<block type="display_select_font" colour="#4a6cd4"><field name="FONT">gdisplay.FONT_DEFAULT</field></block>
	<block type="display_write_text" colour="#4a6cd4">
	  <value name="TEXT"><shadow type="text"><field name="TEXT"></field></shadow></value>
	  <value name="X"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	  <value name="Y"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
	</block>
	<block type="display_align_x_sel" colour="#4a6cd4"><field name="ALIGN">gdisplay.RIGHT</field></block>
	<block type="display_align_y_sel" colour="#4a6cd4"><field name="ALIGN">0</field></block>
  </category>
  <category name="Neopixel" colour="#4a6cd4">
	<block type="neopixel_set_pixels" colour="#4a6cd4">
